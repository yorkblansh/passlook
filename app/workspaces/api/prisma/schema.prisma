generator client {
  provider = "prisma-client-js"
  // output   = "./generated/prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          Int    @id @default(autoincrement())
  email       String @unique
  surname     String
  name        String
  secondName  String
  password    String
  position    String
  department  String
  appPassword String
}
